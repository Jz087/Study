package com.ysbn.videotrain.managesystem.vo;

public class OrganizeVo {

	/*
	 * 组织结构
	 */
	private String deparcode;		//部门代码
	private String level;			//层级，1公司级，2部门级，3科室级
	private String belong;			//归属
	private String deparname;		//部门名称
	private String useflag;			//在用标志
	public String getDeparcode()
	{
		return deparcode;
	}
	public void setDeparcode(String deparcode)
	{
		this.deparcode = deparcode;
	}
	public String getLevel()
	{
		return level;
	}
	public void setLevel(String level)
	{
		this.level = level;
	}
	public String getBelong()
	{
		return belong;
	}
	public void setBelong(String belong)
	{
		this.belong = belong;
	}
	public String getDeparname()
	{
		return deparname;
	}
	public void setDeparname(String deparname)
	{
		this.deparname = deparname;
	}
	public String getUseflag()
	{
		return useflag;
	}
	public void setUseflag(String useflag)
	{
		this.useflag = useflag;
	}
	
}
